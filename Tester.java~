public class Tester {
 
  public Static void main ( String[] args ) {
    
    // Test the bikes
    Bicycle b = new Bicycle();
    MTB m = new MTB();
    
    b.speedUp( 24 );
    m.speedUP( 34 );
    
    System.out.println( "The bike is going " + b.getSpeed() + " km/hr" );
    System.out.println( "The mtn bike is going " + b.getSpeed() + " km/hr" );
    // Test the shapes
    Circle c = new Circle();
    Cylinder cy = new Cylinder();
    
  } // End main method
  
} // End class